@model IEnumerable<EC.Venta>

@{
    Layout = "~/Views/Shared/_LayoutMenu.cshtml";
}

<h2>Ventas de Articulo</h2>

@ViewBag.Mensaje

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VenNum)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VenFec)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.VenNum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VenFec)
            </td>
        </tr>
    }

</table>
<p>
    @Html.ActionLink("Volver al Listado", "ListadoInteractivoArticulos", "Articulo")
</p>